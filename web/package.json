    "axios": "^1.13.2",
    "gsap": "^3.14.2",
    "immer": "^11.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
